<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="WeatherApiApp.Views.WeatherCurrentPage"
             Title="WeatherCurrentPage">
    <StackLayout>
        <Label Text="ASDZXC" />
        <Label Text="{Binding WindSpeed.Name}" IsVisible="{Binding WindSpeed.Visibility}"/>
        <Label Text="{Binding WindSpeed.Value}" IsVisible="{Binding WindSpeed.Visibility}" />
    </StackLayout>
</ContentPage>